{
  "manifest_version": 3,
  "name": "Cookie Exporter",
  "version": "3.0",
  "description": "Exports cookies for personal use.",
  "host_permissions": [
    "*://*.zerodha.com/*"
  ],
  "permissions": [
    "cookies",
    "activeTab",
    "downloads"
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }}